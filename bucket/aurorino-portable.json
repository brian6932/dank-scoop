            "hash": "ef328c69acd1fb790e1134135885b09888c24f1475bf07cf3caa7b7af48b314a",
            "pre_install": [
                "Expand-7zipArchive \"$dir\\chatterino-windows-x86-64.zip\" \"$dir\" -ExtractDir 'Chatterino2' -Removal",
                "Get-ChildItem \"$dir\\chatterino.exe\" | Rename-Item -NewName { $_.name.Replace('chatterino', 'aurorino-portable') }",
                "$modes = \"$dir\\modes\"",
                "$PSDefaultParameterValues['Out-File:Encoding'] = 'utf8'",
                "Invoke-Expression \"'portable' >$(if (Test-Path $modes) { '>' }) `$modes\""
            ]
        }
    },
    "persist": [
        "Logs",
        "Misc",
        "Settings"
    ],
    "shortcuts": [
        [
            "aurorino-portable.exe",
            "Aurorino-portable"
        ]
    ],
    "bin": "aurorino-portable.exe",
